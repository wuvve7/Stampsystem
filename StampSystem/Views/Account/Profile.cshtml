@model StampSystem.Models.ApplicationUser

@{
    ViewData["Title"] = "الملف الشخصي";
}

<h2>الملف الشخصي</h2>

<div class="card p-3">
    <p><strong>الاسم:</strong> @Model.FullName</p>
    <p><strong>البريد:</strong> @Model.Email</p>
    <p><strong>الهاتف:</strong> @Model.PhoneNumber</p>
    <p><strong>الهوية:</strong> @Model.NationalID</p>
    <p><strong>الإدارة:</strong> @Model.AdministrationId</p>
    <p><strong>الدور:</strong> @Model.Role</p>
</div>
